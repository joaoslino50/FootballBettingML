<?xml version="1.0"?>
<rdf:RDF xmlns="http://striko.football/betting/ontology#"
     xml:base="http://striko.football/betting/ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:striko="http://striko.football/betting/"
     xmlns:schema="http://schema.org/">
    
    <owl:Ontology rdf:about="http://striko.football/betting/ontology">
        <rdfs:label>Football Betting Ontology</rdfs:label>
        <rdfs:comment>Ontology for football betting predictions with uncertainty quantification. Designed for the Stiko ML project.</rdfs:comment>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>
    
    <!-- Classes -->
    
    <owl:Class rdf:about="http://striko.football/betting/Team">
        <rdfs:subClassOf rdf:resource="http://schema.org/SportsTeam"/>
        <rdfs:label>Team</rdfs:label>
        <rdfs:comment>A football team that participates in matches.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://striko.football/betting/Match">
        <rdfs:subClassOf rdf:resource="http://schema.org/SportsEvent"/>
        <rdfs:label>Match</rdfs:label>
        <rdfs:comment>A football match between two teams.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://striko.football/betting/BettingOdds">
        <rdfs:label>BettingOdds</rdfs:label>
        <rdfs:comment>Betting odds for a specific match from bookmakers.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://striko.football/betting/Prediction">
        <rdfs:label>Prediction</rdfs:label>
        <rdfs:comment>A machine learning prediction for a betting market outcome.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://striko.football/betting/BettingMarket">
        <rdfs:label>BettingMarket</rdfs:label>
        <rdfs:comment>A type of betting market (e.g., match result, over/under 2.5, BTTS).</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://striko.football/betting/UncertaintyInterval">
        <rdfs:label>UncertaintyInterval</rdfs:label>
        <rdfs:comment>A confidence interval representing prediction uncertainty from UQ360.</rdfs:comment>
    </owl:Class>
    
    <!-- Object Properties -->
    
    <owl:ObjectProperty rdf:about="http://striko.football/betting/homeTeam">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://striko.football/betting/Team"/>
        <rdfs:label>homeTeam</rdfs:label>
        <rdfs:comment>The home team in a match.</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://striko.football/betting/awayTeam">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://striko.football/betting/Team"/>
        <rdfs:label>awayTeam</rdfs:label>
        <rdfs:comment>The away team in a match.</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://striko.football/betting/forMatch">
        <rdfs:domain rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:domain rdf:resource="http://striko.football/betting/BettingOdds"/>
        <rdfs:range rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:label>forMatch</rdfs:label>
        <rdfs:comment>Links a prediction or odds to its associated match.</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://striko.football/betting/hasPrediction">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:label>hasPrediction</rdfs:label>
        <rdfs:comment>Links a match to its ML predictions.</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://striko.football/betting/hasUncertaintyInterval">
        <rdfs:domain rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:range rdf:resource="http://striko.football/betting/UncertaintyInterval"/>
        <rdfs:label>hasUncertaintyInterval</rdfs:label>
        <rdfs:comment>Links a prediction to its confidence interval from UQ360.</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://striko.football/betting/forMarket">
        <rdfs:domain rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:range rdf:resource="http://striko.football/betting/BettingMarket"/>
        <rdfs:label>forMarket</rdfs:label>
        <rdfs:comment>Links a prediction to its betting market type.</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- Data Properties -->
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/hasEloRating">
        <rdfs:domain rdf:resource="http://striko.football/betting/Team"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>hasEloRating</rdfs:label>
        <rdfs:comment>The Elo rating of a team.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/homeElo">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>homeElo</rdfs:label>
        <rdfs:comment>Elo rating of the home team at match time.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/awayElo">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>awayElo</rdfs:label>
        <rdfs:comment>Elo rating of the away team at match time.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/form3Home">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>form3Home</rdfs:label>
        <rdfs:comment>Home team's form over last 3 matches.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/form5Home">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>form5Home</rdfs:label>
        <rdfs:comment>Home team's form over last 5 matches.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/form3Away">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>form3Away</rdfs:label>
        <rdfs:comment>Away team's form over last 3 matches.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/form5Away">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>form5Away</rdfs:label>
        <rdfs:comment>Away team's form over last 5 matches.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/homeGoals">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>homeGoals</rdfs:label>
        <rdfs:comment>Goals scored by home team.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/awayGoals">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>awayGoals</rdfs:label>
        <rdfs:comment>Goals scored by away team.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/totalGoals">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>totalGoals</rdfs:label>
        <rdfs:comment>Total goals in match.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/result">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>result</rdfs:label>
        <rdfs:comment>Match result: H (home win), D (draw), A (away win).</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/over25">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label>over25</rdfs:label>
        <rdfs:comment>Whether match had over 2.5 total goals.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/btts">
        <rdfs:domain rdf:resource="http://striko.football/betting/Match"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label>btts</rdfs:label>
        <rdfs:comment>Whether both teams scored.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- Betting Odds Properties -->
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/homeOdds">
        <rdfs:domain rdf:resource="http://striko.football/betting/BettingOdds"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>homeOdds</rdfs:label>
        <rdfs:comment>Decimal odds for home team win.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/drawOdds">
        <rdfs:domain rdf:resource="http://striko.football/betting/BettingOdds"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>drawOdds</rdfs:label>
        <rdfs:comment>Decimal odds for draw.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/awayOdds">
        <rdfs:domain rdf:resource="http://striko.football/betting/BettingOdds"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>awayOdds</rdfs:label>
        <rdfs:comment>Decimal odds for away team win.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/impliedProbHome">
        <rdfs:domain rdf:resource="http://striko.football/betting/BettingOdds"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>impliedProbHome</rdfs:label>
        <rdfs:comment>Implied probability of home win from odds (1/odds).</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/impliedProbDraw">
        <rdfs:domain rdf:resource="http://striko.football/betting/BettingOdds"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>impliedProbDraw</rdfs:label>
        <rdfs:comment>Implied probability of draw from odds.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/impliedProbAway">
        <rdfs:domain rdf:resource="http://striko.football/betting/BettingOdds"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>impliedProbAway</rdfs:label>
        <rdfs:comment>Implied probability of away win from odds.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- Prediction Properties -->
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/predictedOutcome">
        <rdfs:domain rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>predictedOutcome</rdfs:label>
        <rdfs:comment>The predicted outcome (e.g., H, D, A, true, false).</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/probability">
        <rdfs:domain rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>probability</rdfs:label>
        <rdfs:comment>The predicted probability (0.0 to 1.0).</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/market">
        <rdfs:domain rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>market</rdfs:label>
        <rdfs:comment>The betting market type (match_result, over_under_25, btts, clean_sheet_home, clean_sheet_away).</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/modelName">
        <rdfs:domain rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>modelName</rdfs:label>
        <rdfs:comment>Name of the ML model that made this prediction.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/predictionDate">
        <rdfs:domain rdf:resource="http://striko.football/betting/Prediction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>predictionDate</rdfs:label>
        <rdfs:comment>When the prediction was made.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- Uncertainty Interval Properties (UQ360) -->
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/uncertaintyLower">
        <rdfs:domain rdf:resource="http://striko.football/betting/UncertaintyInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>uncertaintyLower</rdfs:label>
        <rdfs:comment>Lower bound of 90% confidence interval.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/uncertaintyUpper">
        <rdfs:domain rdf:resource="http://striko.football/betting/UncertaintyInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>uncertaintyUpper</rdfs:label>
        <rdfs:comment>Upper bound of 90% confidence interval.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/uncertaintyStd">
        <rdfs:domain rdf:resource="http://striko.football/betting/UncertaintyInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>uncertaintyStd</rdfs:label>
        <rdfs:comment>Standard deviation of prediction uncertainty.</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://striko.football/betting/confidenceLevel">
        <rdfs:domain rdf:resource="http://striko.football/betting/UncertaintyInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>confidenceLevel</rdfs:label>
        <rdfs:comment>Confidence level of the interval (default 0.9 for 90%).</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- Betting Market Individuals -->
    
    <owl:NamedIndividual rdf:about="http://striko.football/betting/MatchResultMarket">
        <rdf:type rdf:resource="http://striko.football/betting/BettingMarket"/>
        <rdfs:label>Match Result (1X2)</rdfs:label>
        <rdfs:comment>Predict match result: Home win (H), Draw (D), or Away win (A).</rdfs:comment>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://striko.football/betting/OverUnder25Market">
        <rdf:type rdf:resource="http://striko.football/betting/BettingMarket"/>
        <rdfs:label>Over/Under 2.5 Goals</rdfs:label>
        <rdfs:comment>Predict whether total goals will be over or under 2.5.</rdfs:comment>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://striko.football/betting/BTTSMarket">
        <rdf:type rdf:resource="http://striko.football/betting/BettingMarket"/>
        <rdfs:label>Both Teams To Score</rdfs:label>
        <rdfs:comment>Predict whether both teams will score at least one goal.</rdfs:comment>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://striko.football/betting/CleanSheetHomeMarket">
        <rdf:type rdf:resource="http://striko.football/betting/BettingMarket"/>
        <rdfs:label>Home Team Clean Sheet</rdfs:label>
        <rdfs:comment>Predict whether home team will keep a clean sheet.</rdfs:comment>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://striko.football/betting/CleanSheetAwayMarket">
        <rdf:type rdf:resource="http://striko.football/betting/BettingMarket"/>
        <rdfs:label>Away Team Clean Sheet</rdfs:label>
        <rdfs:comment>Predict whether away team will keep a clean sheet.</rdfs:comment>
    </owl:NamedIndividual>

</rdf:RDF>

